{
  "expo": {
    "name": "Shopping List",
    "slug": "shopping-list-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Enhance a black silh.png",
    "scheme": "shoppinglist",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.shoppinglist.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#2E7D32",
        "foregroundImage": "./assets/images/Enhance a black silh.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.shoppinglist.app"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/Enhance a black silh.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Enhance a black silh.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#2E7D32",
          "dark": {
            "image": "./assets/images/Enhance a black silh.png",
            "backgroundColor": "#1B5E20"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
